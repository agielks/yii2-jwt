{
    "name": "agielks/yii2-jwt",
    "description": "JWT based on Icobucci version 4.1",
	"type": "yii2-extension",
	"keywords": ["yii2", "yii 2", "jwt"],
	"authors": [
        {
            "name": "Agiel K. Saputra",
            "email": "agielkurniawans@gmail.com"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.0",
        "lcobucci/jwt": "~4.1.0",
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.0"
    },
    "autoload": {
        "psr-4": {
            "agielks\\yii2\\jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "agielks\\yii2\\jwt\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
